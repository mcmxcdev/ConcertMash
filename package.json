{
  "name": "concertmash",
  "description": "Easily generate a playlist for your upcoming concerts based on selected artists!",
  "version": "1.0.0",
  "author": "MCMXC",
  "license": "MIT",
  "homepage": "https://github.com/mcmxcdev/ConcertMash",
  "scripts": {
    "clean": "rm -rf .svelte-kit && rm -rf build",
    "dev": "vite dev",
    "validate": "svelte-kit sync && svelte-check",
    "check:format": "prettier --write src/**/*{.svelte,.ts}",
    "check:types": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix --report-unused-disable-directives",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@beyonk/svelte-notifications": "^4.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@sveltejs/adapter-static": "1.0.0-next.48",
    "@sveltejs/kit": "1.0.0-next.571",
    "@tailwindcss/forms": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.0",
    "cssnano": "^5.1.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-unicorn": "^45.0.1",
    "filepond": "^4.30.4",
    "filepond-plugin-file-encode": "^2.1.10",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "filepond-plugin-file-validate-type": "^1.2.8",
    "filepond-plugin-image-exif-orientation": "^1.0.11",
    "filepond-plugin-image-preview": "^4.6.11",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "p-limit": "^4.0.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "spotify-web-api-js": "^1.5.2",
    "svelte": "^3.53.1",
    "svelte-awesome": "^3.0.1",
    "svelte-check": "^2.10.0",
    "svelte-filepond": "^0.2.0",
    "svelte-forms-lib": "^2.0.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-select": "^4.4.7",
    "svelte-simple-modal": "^1.4.5",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "yup": "^0.32.11"
  },
  "type": "module"
}
