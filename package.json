    "@sveltejs/kit": "1.0.0-next.302",
    "@tailwindcss/forms": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "cssnano": "^5.1.5",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
